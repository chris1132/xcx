<view class="page">
  <view class="section">
    <input placeholder="输入起始地址"  type="text" bindinput="bindKeyInput" data-id="0" value='{{startadd}}'/>
    <input placeholder="输入目的地址"  type="text" bindinput="bindKeyInput" data-id="1" value="{{endadd}}"/>
  </view>

  <view>
    <view class="weui-cells weui-cells_after-title">
      <view wx:for="{{dataAry}}"  class="weui-cell" wx:key="unique" >
        <view bindtap='selectIndex' data-id="{{index}}">
            <view class="weui-cell__bd">{{item.title}}</view>
            <view class="weui-cell__ft">{{item.address}}</view>
        </view>
      </view>
    </view>
  </view>

<view class="map_container">
   <map
      id="myMap" style="width: 100%; height: 100%;" latitude="{{latitude}}" longitude="{{longitude}}" markers='{{markers}}' show-location="true" include-points="{{includePoints}}"
    ></map>
</view> 

</view>
